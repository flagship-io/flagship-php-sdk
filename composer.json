{
  "name": "flagship-io/flagship-php-sdk",
  "type": "library",
  "description": "",
  "require": {
    "php": ">=5.4",
    "psr/log": "^1.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Flagship\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flagship\\": "tests"
    }
  },
  "bin": ["bin/flagship"],
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs --standard=PSR12 -n src tests",
    "fix-style": "phpcbf --standard=PSR12 src tests"
  },
  "license": "MIT"
}
